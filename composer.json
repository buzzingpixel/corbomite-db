{
    "name": "buzzingpixel/corbomite-db",
    "description": "Corbomite DB",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "ext-pdo": "*",
        "atlas/orm": "^3.1",
        "atlas/cli": "^2.2",
        "buzzingpixel/corbomite-di": "^1.0",
        "buzzingpixel/corbomite-cli": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\db\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
