{
    "name": "buzzingpixel/corbomite-db",
    "description": "Corbomite DB",
    "version": "1.3.1",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "ext-pdo": "*",
        "atlas/orm": "^3.1",
        "atlas/cli": "^2.2",
        "buzzingpixel/corbomite-di": "^1.0",
        "buzzingpixel/corbomite-cli": "^1.0",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\db\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php",
        "cliActionConfigFilePath": "src/actionConfig.php"
    }
}
